  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-proxy": "ng serve --proxy-config proxy-conf.json",
    "build": "ng build",
    "build-fg": "ng build --prod --base-href=/fr/",
    "build-fr": "ng build --base-href=/fr/",
    "watch": "ng build --watch --configuration development",
